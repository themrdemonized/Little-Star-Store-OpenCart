  <style>
    .image-test {
      min-height: 100px;
      background: #EEE;
      margin: 15px;
      border-radius: 3px;
      border: 1px solid #CCC;
      position: relative;
      overflow: hidden;
    }
    .image-test img {
      position: absolute;
    }
  </style>
  <script>
    var imgh = 0;
    var imgw = 0;
    var started = false;
    function recalc() {
      if(!imgw) imgw = $('.image-test img').width();
      if(!imgh) imgh = $('.image-test img').height();
      var zoom = $('select[name=config_watermark_zoom]').val();
      var resize_first = parseInt($('select[name=config_watermark_resize_first]').val());
      var pos_x = parseInt($('input[name="config_watermark_pos_x"]').val());
      var pos_x_center = parseInt($('select[name="config_watermark_pos_x_center"]').val());
      var pos_y = parseInt($('input[name="config_watermark_pos_y"]').val());
      var pos_y_center = parseInt($('select[name="config_watermark_pos_y_center"]').val());
      var opacity = $('select[name=config_watermark_opacity]').val();
      var width = imgw * zoom;
      var height = imgh * zoom;
      var transform = ["0", "0"];

      options = {
          'opacity': opacity,
          'width': (resize_first ? (parseInt($('.image-test').width()) - Math.abs(pos_x) * 2) : width) + "px",
          'height': resize_first ? "initial" : (height + "px"),
        };

      if (resize_first) {
        $('.image-test img').css('right', 'initial');
        options.left = Math.abs(pos_x) + 'px';
        if (pos_y_center) {
          options.top = 'calc(50%' + (pos_y >= 0 ? (' + ' + Math.abs(pos_y)) : (' - ' + Math.abs(pos_y))) + 'px)';
          options.transform = 'translateY(-50%)';
        } else {
          if (pos_y >= 0) {
            options.top = Math.abs(pos_y) + 'px';
            options.bottom = 'initial';
          } else {
            options.bottom = Math.abs(pos_y) + 'px';
            options.top = 'initial';
          }
          options.transform = 'none';
        }
      } else {
        if (pos_x_center) {
          options.left = 'calc(50%' + (pos_x >= 0 ? (' + ' + Math.abs(pos_x)) : (' - ' + Math.abs(pos_x))) + 'px)';
          transform[0] = "-50%";
        } else {
          if (pos_x >= 0) {
            options.left = Math.abs(pos_x) + 'px';
            options.right = 'initial';
          } else {
            options.right = Math.abs(pos_x) + 'px';
            options.left = 'initial';
          }
          transform[0] = "0";
        }
        if (pos_y_center) {
          options.top = 'calc(50%' + (pos_y >= 0 ? (' + ' + Math.abs(pos_y)) : (' - ' + Math.abs(pos_y))) + 'px)';
          transform[1] = '-50%';
        } else {
          if (pos_y >= 0) {
            options.top = Math.abs(pos_y) + 'px';
            options.bottom = 'initial';
          } else {
            options.bottom = Math.abs(pos_y) + 'px';
            options.top = 'initial';
          }
          transform[1] = "0";
        }
        options.transform = 'translate(' + transform.join(",") + ')';
      }   

      $('.image-test img').css(options);
    }

    var imgo = $('.image-test img').attr('src');
    function test_image() {
      if( ("/image/" + $('input[name=config_watermark_image]').val()) != imgo) {
        $('.image-test img').attr('src', "/image/" + $('input[name=config_watermark_image]').val());
        $('.image-test img').css({ width: 'auto', height: 'auto' });
        setTimeout(function() {
          imgw = $('.image-test img').width();
          imgh = $('.image-test img').height();
          imgo = "/image/" + $('input[name=config_watermark_image]').val();
          recalc();
        },500);
      }
    }

    function start() {
      $('.image-test').css('height', Math.max($('.image-test').width(), 500));
      recalc();
      $('.config-watermark select, .config-watermark input').change(function(){ recalc(); });
      $('.config-watermark input').keyup(function(){ recalc(); });
      $(window).resize(function(event) {
        recalc();
      });
      setInterval("test_image();", 200);
    }

    $('.tab-ecos').click(function() {
      if (!started) {
        setTimeout('start();', 100);
        started = true;
      }
    });
  </script>
