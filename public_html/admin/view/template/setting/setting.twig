                <label class="col-sm-2 control-label" for="input-schema-address"><span data-toggle="tooltip" data-container="#tab-general" title="{{ help_schema_address }}">{{ entry_schema_address }}</span></label>
                <div class="col-sm-10">
                  <textarea name="config_schema_address" rows="5" placeholder="{{ entry_schema_address }}" id="input-schema-address" class="form-control">{{ config_schema_address }}</textarea>
                </div>
              </div>
              {% if locations %}
              <div class="form-group">
                <label class="col-sm-2 control-label"><span data-toggle="tooltip" data-container="#tab-general" title="{{ help_location }}">{{ entry_location }}</span></label>
                <div class="col-sm-10"> {% for location in locations %}
                  <div class="checkbox">
                    <label> {% if location.location_id in config_location %}
                      <input type="checkbox" name="config_location[]" value="{{ location.location_id }}" checked="checked" />
                      {{ location.name }}
                      {% else %}
                      <input type="checkbox" name="config_location[]" value="{{ location.location_id }}" />
                      {{ location.name }}
                      {% endif %} </label>
                  </div>
                  {% endfor %} </div>
              </div>
              {% endif %} </div>
            <div class="tab-pane" id="tab-contacts">
              <ul class="nav nav-tabs" id="contacts-tabs">
                {% for key, group in config_contacts_data %}
                  {% if key == 0 %}
                    <li class="active"><a href="#tab-config_contacts_data-{{ key }}" data-toggle="tab">{{ tab_contacts_all }}</a></li>
                  {% else %}
                    <li><a href="#tab-config_contacts_data-{{ key }}" data-toggle="tab">{{ contact_customer_groups[key].name }}</a></li>
                  {% endif %}
                {% endfor %}
              </ul>
              <div class="tab-content">
                {% for key, group in config_contacts_data %}
                  <div class="tab-pane {% if key == 0 %} active {% endif %}" id="tab-config_contacts_data-{{ key }}">
                    {% for data_key, data in group %}
                      {% if data_key == "addresses" %}
                        <legend><span>{{ contacts_addresses }}</span> <span><button type="button" data-toggle="tooltip" title="{{ button_add }}" class="btn btn-primary btn-address-add"><i class="fa fa-plus"></i></button></span></legend>
                        <div class="form-contacts-addresses">
                          {% for address_key, address in data %}
                            <div class="form-contacts-address form-contacts-address-{{ address_key }}" data-key="{{ address_key }}">
                              {% if address_key != 0 %}
                                <button type="button" data-toggle="tooltip" title="{{ button_delete }}" class="btn btn-danger btn-remove-address btn-remove-address-{{ address_key }}" onclick="confirm('{{ text_confirm }}') ? $('#tab-contacts .tab-pane.active .form-contacts-address-{{ address_key }}').remove() : false;"><i class="fa fa-trash-o"></i></button>
                              {% endif %}
                              {% for address_field, address_data in address %}
                                <div class="form-group">
                                  <label class="col-sm-2 control-label" for="input-contacts-{{ key }}-{{ data_key }}-{{ address_key }}-{{ address_field }}">{% if address_data.tooltip %}<span data-toggle="tooltip" title="{{ address_data.tooltip }}">{% endif %}{{ address_data.label }}{% if address_data.tooltip %}</span>{% endif %}</label>
                                  <div class="col-sm-10">
                                    {% if address_data.type == "textarea" %}
                                     <textarea name="config_contacts_data[{{ key }}][{{ data_key }}][{{ address_key }}][{{ address_field }}][value]" placeholder="{{ address_data.label }}" id="input-contacts-{{ key }}-{{ data_key }}-{{ address_key }}-{{ address_field }}" class="form-control">{{ address_data.value }}</textarea>
                                    {% else %}
                                      <input type="text" name="config_contacts_data[{{ key }}][{{ data_key }}][{{ address_key }}][{{ address_field }}][value]" value="{{ address_data.value }}" placeholder="{{ address_data.label }}" id="input-contacts-{{ key }}-{{ data_key }}-{{ address_key }}-{{ address_field }}" class="form-control" />
                                    {% endif %}
                                  </div>
                                </div>
                              {% endfor %}
                            </div>
                          {% endfor %}
                        {% else %}
                          <div class="form-group">
                            <label class="col-sm-2 control-label" for="input-contacts-{{ key }}-{{ data_key }}">{% if data.tooltip %}<span data-toggle="tooltip" title="{{ data.tooltip }}">{% endif %}{{ data.label }}{% if data.tooltip %}</span>{% endif %}</label>
                            <div class="col-sm-10">
                              {% if data.type == "textarea" %}
                               <textarea name="config_contacts_data[{{ key }}][{{ data_key }}][value]" placeholder="{{ data.label }}" id="input-contacts-{{ key }}-{{ data_key }}" class="form-control">{{ data.value }}</textarea>
                              {% else %}
                                <input type="text" name="config_contacts_data[{{ key }}][{{ data_key }}][value]" value="{{ data.value }}" placeholder="{{ data.label }}" id="input-contacts-{{ key }}-{{ data_key }}" class="form-control" />
                              {% endif %}
                            </div>
                          </div>
                        {% endif %}
                      {% endfor %}
                    </div>
                  </div>
                {% endfor %}
              </div>
            </div>
