  $("body").on("click", ".save-settings", function() {
    var save = $(this);
    var save_html = save.html();
    var form = $("#form-setting");
    $.ajax({
      type: form.attr('method'),
      url: form.attr('action'),
      data: form.serialize(),
      beforeSend: function() {
        save.text('Сохранение').prop('disabled', true);
        console.log('Saving...');
      },
      success: function(json) {
        form.find('.text-danger').each(function() {
          $(this).remove();
        });
        if ($(json).find('.alert-danger').length > 0) {
          console.log('Settings failed');
          $("body").append('<div class="alert alert-danger alert-dismissible alert-fixed">Проверьте форму на ошибки<button type="button" class="close" data-dismiss="alert">&times;</button></div>');
          $(json).find('.text-danger').each(function() {
            var tab = $(this).parents('.tab-pane').attr('id');
            form.find('a[href=#' + tab + ']').css('outline', '1px solid red');
          });
        } else {
          $("body").append('<div class="alert alert-success alert-dismissible alert-fixed">Настройки сохранены<button type="button" class="close" data-dismiss="alert">&times;</button></div>');
          form.find('a[data-toggle=\"tab\"]').each(function() {
            $(this).css('outline', 'initial');
          });
        }
        save.html(save_html).prop('disabled', false);
        setTimeout(function() {
          $('.alert-fixed').animate({opacity: 0}, 500, function() {
            $(this).remove();
          });
        }, 1500);
      },
      error: function(xhr, ajaxOptions, thrownError) {
        alert(thrownError + "\r\n" + xhr.statusText + "\r\n" + xhr.responseText);
      }
    });
  });
