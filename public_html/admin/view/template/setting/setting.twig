<script>

  $('.btn-address-add').click(function() {
    var max_key = -1;
    var el;

    $('#tab-contacts .tab-pane.active .form-contacts-addresses > .form-contacts-address').each(function(index, i) {
      var key = $(i).data('key');
      if (key > max_key) {
        max_key = key;
        el = $(i).clone();
      }
    });

    var new_max_key = max_key + 1;
    $(el).removeClass().addClass('form-contacts-address').addClass('form-contacts-address-' + new_max_key);
    $(el).attr('data-key', new_max_key);

    if ($(el).find('.btn-remove-address').length < 1) {
      $(el).prepend('<button type="button" data-toggle="tooltip" title="{{ button_delete }}" class="btn btn-danger btn-remove-address btn-remove-address-' + new_max_key +'" onclick="confirm(\'{{ text_confirm }}\') ? $(\'#tab-contacts .tab-pane.active .form-contacts-address-' + new_max_key + '\').remove() : false;"><i class="fa fa-trash-o"></i></button>');
    }

    $(el).find('*').each(function(index, i) {
      $(i).val('');

      var attr_for = $(i).attr('for');
      if (attr_for) {
        attr_for = attr_for.replaceAll('addresses-' + max_key, 'addresses-' + new_max_key);
        $(i).attr('for', attr_for);
      }

      var attr_id = $(i).attr('id');
      if (attr_id) {
        attr_id = attr_id.replaceAll('addresses-' + max_key, 'addresses-' + new_max_key);
        $(i).attr('id', attr_id);
      }

      var attr_name = $(i).attr('name');
      if (attr_name) {
        attr_name = attr_name.replaceAll('[addresses][' + max_key + ']', '[addresses][' + new_max_key + ']');
        $(i).attr('name', attr_name);
      }

      if ($(i).hasClass('btn-remove-address-' + max_key)) {
        $(i).removeClass('btn-remove-address-' + max_key).addClass('btn-remove-address-' + new_max_key);
        var attr_onclick = $(i).attr('onclick');
        attr_onclick = attr_onclick.replaceAll('address-' + max_key, 'address-' + new_max_key);
        $(i).attr('onclick', attr_onclick);
      }
    });

    $('#tab-contacts .tab-pane.active .form-contacts-addresses').append(el);

  });
</script>
